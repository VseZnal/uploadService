protoc -I .  --go_out ./services/api-gateway/proto/upload-service --go_opt paths=source_relative  --go-grpc_out ./services/api-gateway/proto/upload-service --go-grpc_opt paths=source_relative  proto/upload.proto

protoc -I .  --go_out ./services/upload-service/proto/upload-service --go_opt paths=source_relative  --go-grpc_out ./services/upload-service/proto/upload-service --go-grpc_opt paths=source_relative  proto/upload.proto

protoc -I . --validate_out=paths=source_relative,lang=go:. backend/proto/album.proto
